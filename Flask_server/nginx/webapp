server {
    listen 80;
    server_name 34.140.211.10 team3.ua-ppdb.me;
    location / {
        include proxy_params;
        proxy_pass http://unix:/home/app/PDTeam3/Flask_server/src/ProgDBTutor/webapp.sock;
    }

    location /socket.io {
        #include proxy_params;
        proxy_http_version 1.1;
        #proxy_buffering off;
        proxy_set_header Upgrade $http_upgrade;
        proxy_set_header Connection "Upgrade";
        proxy_pass http://unix:/home/app/PDTeam3/Flask_server/src/ProgDBTutor/webapp.sock/socket.io;
    }
}